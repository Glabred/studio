{
  "name": "capsul",
  "version": "0.1.0",
  "description": "capsul website",
  "repository": "git@github.com:capsulstudio/studio.git",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "source": "src/index.html",
  "scripts": {
    "start": "npm-run-all --parallel 'watch:*'",
    "watch:start": "parcel serve src/index.html",
    "watch:css": "tailwindcss -i src/assets/styles/index.css -o src/assets/styles/app.css -w",
    "build": "parcel build"
  },
  "dependencies": {
    "@hotwired/stimulus": "^3.0.1",
    "@stripe/stripe-js": "^1.22.0",
    "calendar-js": "^1.4.4",
    "tailwindcss": "^3.0.1",
    "timekit-sdk": "^1.19.3"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.0.1",
    "postcss": "^8.4.4",
    "posthtml-include": "^1.7.2"
  }
}
